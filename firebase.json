{
  "hosting": {
    "public": "src/frontend/dist/ingoodhands",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source": "index.html",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=0, must-revalidate"
      } ]
    }
    ]
  }
}
